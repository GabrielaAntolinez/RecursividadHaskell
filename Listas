--Invertir lista
invert::[Int]->[Int]
invert [ ] = [ ]
invert (x:xs) = (invert xs)++[x]

--Suma de elementos de lista
suma::Int->Int->Int
suma ab=a+b
reduce suma(x:xs)

--Cantidad de impares en una lista

cantimparf::[Int]->Int
cantimparf []=0
filter 

cantimpar::[Int]->Int
cantimpar []=0
cantimpar lista= length [x | x <- lista, mod x 2 ==1]

--Valor Mayor de una lista
mayor::[Int]->Int
mayor (x:xs)
  if x > mayor(xs) then x
    else mayor(xs)
    
